<?xml version="1.0" encoding="utf-8" ?>
<SCF>  

<Service name="mimi" id="116" maxThreadCount="50">
	<Commmunication >
		<!--
	        bufferSize:网络传输流缓存大小
	        minPoolSize:连接池中最小连接数量
	        maxPoolSize:连接池中最大连接数量
	        nagle:是否启用Nagle算法
	        autoShrink:是否启动连接自动回收
	        sendTimeout:发送超时
	        receiveTimeout:接收超时
	        waitTimeout:当连接池中没有连接时需要等待释放连接的时间
      	-->
    	<SocketPool bufferSize="4096" minPoolSize="3" maxPoolSize="100" nagle="true" autoShrink="00:00:20" sendTimeout="00:00:05" receiveTimeout="00:00:10" waitTimeout="00:00:03" />
    	<!--
	        serialize:序列化方式
	        encoder:序列号采用的编码
	        compressType:是否压缩
      	-->
      	<Protocol serialize="SCF" encoder="UTF-8" compressType="UnCompress" />
    </Commmunication>
	<Loadbalance>
		<!--
	        deadTimeout:服务器挂后心跳检测间隔时间
	        maxCurrentUser:最大当前用户数，超过此数服务器将置为busy状态
      	-->
    	<Server deadTimeout="00:00:20">
    		<add name="enterprise" host="127.0.0.1" port="19000" maxCurrentUser="100" />
        	<!--add name="enterprise" host="10.9.18.135" port="19000" maxCurrentUser="100" /-->
		</Server>
    </Loadbalance>
</Service>
<!--
All,Debug,Info,Notice,Warn,Error,Fatal,Off
-->
	<Logger level="Info" type="WWW58COM.SPAT.SCF.Client.Utility.Logger.UDPLogger">
    	<UDP host="www.dns.org" port="8888"/>
    	<File path="scf_log"/>
	</Logger>
</SCF>